{
    "opts": {
        "destination": "docs",
        "recurse": true,
        "template": "node_modules/better-docs"
      },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "plugins": [
        "node_modules/better-docs/typescript",
        "node_modules/better-docs/category"
    ],
    "source": {
        "include": [""],
        "includePattern": "\\.(jsx|js|ts|tsx)$",
        "exclude": [ "node_modules", ".next", ".vscode", "docs" ]
    },
    "templates": {
        "search": true,
        "better-docs": {
            "name": "Trends Near You Documentation",
            "title": "Trends Near You Documentation"
        }
    }

}